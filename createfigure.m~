function createfigure(X1, ymatrix1)
%CREATEFIGURE(X1,YMATRIX1)
%  X1:  area x
%  YMATRIX1:  area matrix data

%  Auto-generated by MATLAB on 15-Jul-2014 14:22:53

% Create figure
figure1 = figure;

% Create axes
axes1 = axes('Parent',figure1);
%% Uncomment the following line to preserve the X-limits of the axes
% xlim(axes1,[1377335427313 1378330569000]);
% xlim(axes1,[1377336221000 1377343122000]);
%xlim(axes1,[0 7694687]);

%% Uncomment the following line to preserve the Y-limits of the axes
ylim(axes1,[0 1]);
box(axes1,'on');
hold(axes1,'all');

% Create title
title('Area Chart of Answer Types');

% Create multiple lines using matrix input to area
area1 = area(X1,ymatrix1,'Parent',axes1);
set(area1(1),'DisplayName','CC');
set(area1(2),'DisplayName','CD');
set(area1(3),'DisplayName','IC');
set(area1(4),'DisplayName','ID');

% Create xlabel
xlabel('Time Passed (ms)');

ylabel('Percent');

% Create legend
legend(axes1,'show');

